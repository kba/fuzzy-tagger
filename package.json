{
  "name": "fuzzy-tagger",
  "version": "0.0.1",
  "description": "",
  "scripts": {
    "test": "coffee test/*.coffee | tap-spec",
    "docs": "docker -i src -o apidocs -s -n -c vs",
    "compile": "coffee -c -o lib src",
    "clean": "rm -r apidocs lib ; true",
    "prepublish": "npm run compile",
    "watch": "watchify client/app.coffee -o public/app.js -v --debug -t coffeeify --extension='.coffee'",
    "serve": "nodemon --exec coffee bin/run-server src/* templates/*.jade",
    "run": "npm run watch & npm run serve"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kba/fuzzy-tagger"
  },
  "author": "kba",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kba/fuzzy-tagger/issues"
  },
  "homepage": "https://github.com/kba/fuzzy-tagger",
  "dependencies": {
    "async": "^0.9.0",
    "backbone": "^1.1.2",
    "body-parser": "^1.12.0",
    "bootstrap": "^3.3.2",
    "coffeeify": "^1.0.0",
    "cson": "^2.0.0",
    "express": "^4.12.2",
    "fuzzy": "^0.1.0",
    "jade": "^1.9.2",
    "jquery": "^2.1.3",
    "jquery-ui": "^1.10.5",
    "json-patch": "^0.5.0",
    "merge": "^1.2.0",
    "superagent": "^1.1.0",
    "watchify": "^2.4.0"
  },
  "devDependencies": {
    "coffee-script": "^1.9.1",
    "docker": "^0.2.14",
    "tap-spec": "^2.2.2",
    "tapes": "^0.4.1"
  },
  "main": "lib/index.js"
}
